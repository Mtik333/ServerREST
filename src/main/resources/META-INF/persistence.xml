<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="TestAuditPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.mycompany.restserv.moviedto.RsiSeat</class>
    <class>com.mycompany.restserv.moviedto.RsiReservation</class>
    <class>com.mycompany.restserv.moviedto.RsiMovie</class>
    <class>com.mycompany.restserv.moviedto.RsiClient</class>
    <class>com.mycompany.restserv.moviedto.RsiSeatReserved</class>
    <class>com.mycompany.restserv.moviedto.RsiScreening</class>
    <class>com.mycompany.restserv.moviedto.RsiAuditorium</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/baza2"/>
      <property name="javax.persistence.jdbc.user" value="app"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="app"/>
    </properties>
  </persistence-unit>
</persistence>
